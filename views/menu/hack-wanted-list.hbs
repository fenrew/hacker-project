<h1>WANTED LIST</h1>

<table cellspacing="0">
  <tr>
    <th>Name</th>
    <th>Alliance</th>
    <th>Rank</th>
    <th>Bounty</th>
    <th>Hack</th>
  </tr>
  {{#each bountyUsers}}
  <tr>
    <td>{{this.name}} </td>
    <td>{{this.alliance}} </td>
    <td>{{this.rankName}} </td>
    <td>${{this.bounty}} </td>
    <td>
      <form action="/hack/hack-player/:{{this.id}}">
        <button type="submit" class="button">Hack!</button>
      </form>
    </td>
  </tr>
  {{/each}}
</table>