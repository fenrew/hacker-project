<h1>MARKETPLACE</h1>

<!-- buy function instead this.buy -->

<table cellspacing="0">
  <tr>
    <th>Name</th>
    <th>Type</th>
    <th>Price</th>
    <th>Bonus</th>
  </tr>

  
  
  {{#each items}}

  <tr>
    <td>{{this.name}} </td>
    <td>{{this.type}} </td>
    <td>{{this.price}} </td>
    <td>{{this.bonus}} </td>
    <td><form action="" method="POST">
                <button class="button-buy">BUY</button></td>
  </tr>
  {{/each}}
  
</table>

